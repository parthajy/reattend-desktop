{
  "$schema": "https://raw.githubusercontent.com/nicedoc/tauri/tauri-v2/packages/cli/schema.json",
  "productName": "Reattend",
  "version": "0.1.0",
  "identifier": "com.reattend.desktop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "node scripts/prebuild.cjs"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "13.0"
    },
    "windows": {
      "webviewInstallMode": {
        "type": "downloadBootstrapper"
      }
    },
    "category": "public.app-category.productivity"
  },
  "plugins": {
  }
}
